<fest:template xmlns:fest="http://fest.mail.ru" context_name="scoreboard">
  <div class="text-center">
    <h2> Scoreboard </h2>
  </div>
  <row>
    <div class="col-md-4 col-md-offset-4">
      <ol class="list-group">
        <fest:each iterate="scoreboard" index="i" value="score">
          <li class="list-group-item">
            <fest:value> score.name </fest:value>
            <span class="badge">
              <fest:value> score.score </fest:value>
            </span>
          </li>
        </fest:each>
      </ol>
    </div>
  </row>
  <div class="clearfix"></div>
  <div class="text-center">
    <a href="/" class="btn btn-default navigate"> Back </a>
  </div>
</fest:template>
